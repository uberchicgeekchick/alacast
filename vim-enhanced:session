#!/bin/tcsh -f
set my_editor = "`printf "\""${0}"\"" | sed 's/.*\/\([^\.]\+\).*/\1/g'`"
switch ( "${my_editor}" )
case "connectED":
case "gedit":
	breaksw
case "vi":
case "vim":
case "vim-enhanced":
default:
	set my_editor = `printf "%s -p" "vim-enhanced"`
	breaksw
endsw

${my_editor} "./data/xml/opml/ip.tv/technology/networks/podcast.cnet.com.opml" "./data/xml/opml/podcasts/online-culture/ChicGeekChicks.opml" "./data/xml/opml/podcasts/online-culture/geek-culture.opml" "./data/xml/opml/podcasts/technology/networks/podcast.cnet.com.opml" "./data/xml/opml/radiocasts/bbc.co.uk/1xtra.opml" "./data/xml/opml/radiocasts/bbc.co.uk/6music.opml" "./data/xml/opml/radiocasts/bbc.co.uk/asiannetwork.opml" "./data/xml/opml/radiocasts/bbc.co.uk/bbc7.opml" "./data/xml/opml/radiocasts/bbc.co.uk/crossstation.opml" "./data/xml/opml/radiocasts/bbc.co.uk/fivelive.opml" "./data/xml/opml/radiocasts/bbc.co.uk/fivelive.opml.xhtml" "./data/xml/opml/radiocasts/bbc.co.uk/local.opml" "./data/xml/opml/radiocasts/bbc.co.uk/radio4.opml" "./data/xml/opml/radiocasts/bbc.co.uk/worldservice.opml" "./data/xml/opml/radiocasts/community-radio/kepx-90.3-fm.opml" "./data/xml/opml/vodcasts/technology/networks/podcast.cnet.com.opml" "./data/xml/pubDate.check" "./scripts/alacast:catalogs:search:then:fetch.tcsh" "./scripts/alacast:catalogs:search:then:fetch.tcsh"

